# Environment Configuration
# Copy this file to .env and update with your actual values

# Server Configuration
PORT=3000
NODE_ENV=development

# API Configuration
API_BASE_URL=http://localhost:3000/api

# File Paths
ENROLLMENT_LIST_PATH=./config/enrolledParticipants.json
SCORING_CONFIG_PATH=./config/scoringConfig.json

# Security (if needed for future enhancements)
# JWT_SECRET=your-jwt-secret-here
# SESSION_SECRET=your-session-secret-here

# Logging
LOG_LEVEL=info

# Rate Limiting (requests per minute)
RATE_LIMIT_REQUESTS=60

# Cache Configuration (if implemented)
CACHE_TTL=300000

# External API Configuration (if needed)
# GOOGLE_CLOUD_API_KEY=your-api-key-here